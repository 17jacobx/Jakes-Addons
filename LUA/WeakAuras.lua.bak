["unevent"] = "auto",
["customDuration"] = "function()\n    aura_env.castTime = aura_env.castTime or 0\n    local duration = 20\n    local expirationTime = aura_env.castTime+duration\n    \n    return duration, expirationTime\nend\n\n\n\n",
["events"] = "COMBAT_LOG_EVENT_UNFILTERED",
["spellIds"] = {
